----------------------------------------------- */
@import "_reset";

/* =Global
----------------------------------------------- */
@import "_global";

/* SCSS 
---------------------------------------- */
//@import "_font-awesome";
@import "compass/css3";

/* Variables */
$carbon: #505050;
$gray-text: #7b7b7b;
$gray: #a6a6a6;
$light-gray: #e1e1e1;
$lighter-gray: #f5f5f5;
$gold: #b1931c;
$dark-gold: #977d18;
$gold-text: #dcb000;
$maroon: #390202;
$maroon-text: #761313;

/* Mixins */
@mixin group {
	&:after {
		content: "";
		display: table;
		clear: both;
	}
}

// Scott Kellum Image Replacement Method
// URL: http://css-tricks.com/examples/ImageReplacement/
@mixin image-replacement ( $url, $width, $height ) {
	width: $width;
	height: $height;
	background: url($url) no-repeat;
	text-indent: 100%;
	white-space: nowrap;
	overflow: hidden;
}

// Use opacity with hex color without converting to rgba
@mixin background-color ($color, $opacity: 0) {
	background-color: $color;
	@include opacity($opacity);
}

// Learn more link style
@mixin learn-more-link {
	color:#ab8b0a; 
	font-size: 1.1em;
	text-transform: uppercase;
	&:hover {
		text-decoration: underline;
	}
}

// List Item style
@mixin list-item {
	background-color: $lighter-gray;
	border-bottom: 2px solid $light-gray;
	padding: 1rem;
}
/* =Layout
----------------------------------------------- */
.site-header,
.nav-wrapper {
	width: 100%;
}
.top-links,
.main-header,
.site-navigation,
.footer-inner,
.content-wrap {
	width: 98em;
}

/* =Repeatable Pattern
----------------------------------------------- */
.cta-button {
	background-color: #f6f3e5;
	border: 2px solid white;
	color: #ad8f12; // @TODO: check whether ad8f12 is close to gold-text
	display: block;
	float: left;
	font-family: 'Helvetica Neue', 'Helvetica', sans-serif;
	font-size: 1.4rem;
	font-weight: bold;
	padding: 1rem 3.5rem 1rem 2.5rem;
	position: relative;
	text-transform: uppercase;
	@include box-shadow(0 0 2px rgba(0,0,0,0.2), 0 0 1px rgba(0,0,0,0.2) inset);
	&:after {
		content: "";
		background: url(../inc/images/assets.png) no-repeat -36px 0;
		display: block;
		width: 8px;
		height: 11px;
		position: absolute;
		right: 1.8rem;
		bottom: 1.4rem;
	}
}

/* =Header
----------------------------------------------- */
.site-header {
	.top-links {
		height: 3.2em;
		margin: 0 auto;
		padding-left: 5em;
		@media (min-width: 103em) {
			width: 103em;
		}
		box-shadow: 0 5px 6px -4px rgba(0,0,0,0.2);
		li {
			float: left;
			list-style: none;
			margin-left: 5em;
			padding-top: 1rem;
			&:before {
				background: url(../inc/images/assets.png);
				content:  "";
				display: inline-block;
				width: 9px;
				height: 9px;
				margin: 0 0.4em 0.1em 0;
			}
			a {
				color: $gray-text;
				font-size: 1.4em;
				text-transform: uppercase;
			}
			&:nth-child(2),
			&:first-child + li {
				&:before {
					background-position: -9px 0;
				}
			}
			&:nth-child(3),
			&:first-child + li + li {
				&:before {
					background-position: -18px 0;
				}
			}
			&:nth-child(4),
			&:first-child + li + li + li {
				&:before {
					background-position: -27px 0;
				}
			}
		}
	}
	.main-header {
		margin: 3em auto;
		@include group;
		.site-title {
			background: url(../inc/images/logo.png) no-repeat;
			display: block;
			float: left;
			text-indent: -100%;
			width: 387px;
			height: 58px;
		}
		.member-login a {
			//background: #fbfbfb url(../inc/images/login-arrow.png) no-repeat 11px 11px;
			border: 2px solid white;
			box-shadow: inset 0 0 3px rgba(0,0,0,0.1),
						2px 2px 3px rgba(0,0,0,0.2);
			color: $gold;
			float: right;
			font-size: 1.6em;
			line-height: 1;
			padding: 1.5rem 4rem 1rem;
			position: relative; 
			text-transform: uppercase;
			&:hover {
				background-color: #eaeaea;
				text-decoration: none;
			}
			&:before {
				background: url(../inc/images/assets.png) no-repeat 0 -10px;
				content: "";
				display: block;
				width: 22px;
				height: 22px;
				position: absolute;
				top: 10px;
				left: 12px;
			}
		}
	}
}

/* =Menu
----------------------------------------------- */
.nav-wrapper {
	height: 4.6rem;
	background: $gold;
	.site-navigation {
		@include group;
		clear: both;
		display: block;
		margin: 0 auto;
		ul {
			list-style: none;
			margin: 0 0 0 -2rem;
			padding-left: 0;
			ul {
				box-shadow: 0 3px 3px rgba(0,0,0,0.2);
				display: none;
				float: left;
				position: absolute;
				top: 1.5rem;
				left: 0;
				z-index: 99999;
				ul {
					left: 100%;
					top: 0;
				}
				a {
					width: 200px;
				}
			}
		}
		li {
			border-right: 2px solid $dark-gold;
			float: left;
			position: relative;
			&:last-child {
				border: none;
			}
			&:hover {
				background-color: $dark-gold;
				cursor: pointer;
			}
			&.current-menu-item {
				background-color: $dark-gold;
			}
		}
		a {
			color: white;
			font-size: 1.6rem;
			line-height: 1;
			display: block;
			padding: 0.9375em 1.25em; // 15/16 = 0.9375, 20/16 = 1.25
			text-transform: uppercase;
			&:hover {
				text-decoration: none;
			}
		}
	}
}

/* Small menu */
.menu-toggle {
	cursor: pointer;
}
.main-small-navigation .menu {
	display: none;
}

/* =Content
----------------------------------------------- */

/* HOMEPAGE */

@import "_home";

/* Main Standard Content */
.site-main {
	border-bottom: 2px solid $light-gray;
	padding-bottom: 6rem;
	@include group;
}
.cover-photo {
	max-height: 35rem;
	min-height: 5.9em;
	overflow: hidden;
	img {
		width: 100%;
		height: auto;
	}
}

.content-wrap {
	margin: -5.9em auto 0;
	position: relative;
	z-index: 2;
	@include group;
}

.widget-area {
	background-color: #fdfdfd;
	border: 3px solid #f1eee4;
	width: 100%;
	//box-shadow: 0 1px 4px 1px rgba(0,0,0,0.3);
	@include box-shadow(rgba(0,0,0,0.3) 0 1px 4px 1px);
	@media (min-width: 50em) { // min width 800px / 16
		float: left;
		width: 27.5510204%; // 270px / 980px
	}
}
.content-area {
	width: 100%;
	@media (min-width: 50em) { // min width 800px / 16
		box-sizing: border-box;
		float: right;
		width: 71.4285714%; // 700px / 980px
	}
}

.site-content {
	.entry-header {
		@include background-color($maroon, 0.75);
		margin-left: -3px; // To make up for the box-shadow on the sidebar
		padding: 1.5em 0 1em 5.7142857%; //38px / 665px;
		.entry-title {
			color: white;
			font-size: 2.8em;
			text-transform: uppercase;
		}
	}
	.entry-content {
		padding-left: 5.2631579%; // 35px / 665px
		line-height: 1.4;
		h1 {
			color: $maroon-text;
			font-size: 2.8em;
		}
		h2 {
			font-size: 1.9em;
		}
		h3 {
			color: $maroon-text;
			font-size: 1.5rem;
		}
		// add spacing for the subheadline (right after main headline)
		h1+h2 {
			margin-bottom: 2.5rem;
		}
		ul {
			margin-bottom: 1.5rem;
		}
		p,
		li {
			color: $carbon;
			font-size: 1.4em;
			line-height: 1.5;
		}
		a {
			color: $gold-text;
			&:hover {
				text-decoration: underline;
			}
		}
	}
	.presentation {
		background-color: #f5f5f5;
		padding: 1.5rem;
		margin: 0 0 .8rem -1.5rem;
		@include group;
		h1 {
			color: $maroon-text;
			font-size: 1.5rem;
		}
	}
}

.sticky {
}
.hentry {
	margin: 0 0 1.5em;
}
.entry-meta {
	clear: both;
}
.byline {
	display: none;
}
.single .byline,
.group-blog .byline {
	display: inline;
}
.entry-content,
.entry-summary {
	margin: 1.5em 0 0;
}
.page-links {
	clear: both;
	margin: 0 0 1.5em;
}

/* ENERGY FOLKS PAGE */

@import "_energyfolks";

/* SLOAN PAGE */

@import "_sloan";

/* EXECUTIVE COMMITTEE PAGE */

.team-members {
	.team-member {
		margin-bottom: 1rem; 
		@include group;
		@include list-item;
		img {
			@media (min-width: 25em) { // 400px / 16
				max-width: 15%;
				float: left;
			}
			@media (min-width: 60em ) { //960px / 16
				max-width: 100%;
			}
		}
		.member-description {
			float: right;
			@media (min-width: 25em) { // 400px / 16
				float: right;
				width: 70%;
				margin-left: 5%;
			}
			@media (min-width: 60em) {// 960px / 16
				width: 76%;
			}
			h1 {
				font-size: 1.9rem;
			}
		}
	}
}

/* SPONSORS PAGE */

.sponsor-single {
	margin-bottom: 1rem;
	@include list-item;
	img {
		float: left;
		margin: 0 1rem 1rem 0;
	}
	h3 {
		display: inline-block;
	}
}

/* =Asides
----------------------------------------------- */

.blog .format-aside .entry-title,
.archive .format-aside .entry-title {
	display: none;
}

.sidebar-nav {
	margin-bottom: 3.5em;
	text-transform: uppercase;
	h1 {
		color: $gold-text;
		font-size: 1.2em;
		padding: 1.5em 0 1em 7.4074074%; //20px / 270px
	}
	li {
		border-top: 1px solid $light-gray;
		cursor: pointer;
		
		> a {
			color: $gray-text;
			display: block;
			font-size: 1.4rem;
			padding: 1rem 0 1rem 7.4074074%; // 20px / 270px
		}
		&.current_page_item,
		&.current-cat,
		&:hover {
			background-color: $light-gray;
		}
	}
}

/* Tiemly Content */
.timely-content {
	// TO-DO: linear background gradient
	border-top: 1px solid $light-gray;
	line-height: 1.4;
	padding: 1em 0 1em 7.4074074%; // 20px / 270px
	h1 {
		font-size: 1.5em;
		margin-bottom: 1rem;
	}
	img {
		max-width: 85.1851852%; // 230px / 270px
		height: auto;
		margin-bottom: 1em;
	}
	p {
		color: $gray-text;
		margin-bottom: 0.5em;
	}
	a {
		@include learn-more-link;
	}
}

/* Facebook Like Box Plugin */
.facebook-outer {
	border-top: 1px solid $light-gray;
	width:27rem; 
	//height:23.4rem;
	overflow: hidden;
	@include background-image(linear-gradient(#f3f3f3, #fdfdfd));
}

/* =Footer
----------------------------------------------- */
.sponsor-footer-wrap {
	border-bottom: 2px solid $light-gray;
	width: 100%;
}
.sponsor-footer {
	margin: 0 auto;
	width: 98rem;
	@include group;
	h1 {
		color: $maroon-text;
		float: left;
		font-size: 1.5rem;
		padding-top: 5rem;
		text-align: right;
		width: 10.5rem;
		height: 7rem;
	}
	.sponsor-logo {
		cursor: pointer;
		float: left;
		width: 17.5rem;
		height: 12rem;
		background-repeat: no-repeat;
		@media (min-width: 37.5em) { // 600px / 16px
			@include filter(grayscale(100%));
			&:hover {
				@include filter(grayscale(0));
			}
		}
	}
}

.footer-inner {
	margin: 2em auto;
	@include group;
}
.copyright {
	color: $gray-text;
	float: left;
	margin-top: 1em;
}
.sharing {
	float: right;
	@media (min-width: 25em) { // 400px / 16
		min-width: 40em;
	}
	.stay-connected {
		border-right: 1px solid $light-gray;
		font-size: 1.2em;
		margin-right: 1rem;
		padding: 1rem;
		@media (min-width: 25em) {
			float: left;
		}
	}
	.share-button {
		// imageReplacement ($url, $width, $height)
		@include image-replacement("../inc/images/assets.png", 2.6em, 2.6em );
		display: block;
		float: left;
		margin: 0.3em 1em;
		&:last-child {
			margin-right: 0;
		}
	}
	.facebook-button {
		background-position: 0 -68px
	}
	.twitter-button {
		background-position: -27px -68px;
	}
	.linkedin-button {
		background-position: -55px -68px;
	}
	.newsletter-button {
		background-position: -82px -68px;
	}
	.contact-button {
		background-position: -109px -68px;
	}
	.addthis-button {
		background-position: -136px -68px;
	}
}

/* =Media
----------------------------------------------- */

.site-header img,
.entry-content img,
.comment-content img,
.widget img {
	max-width: 100%; /* Fluid images for posts, comments, and widgets */
}
.site-header img,
.entry-content img,
img[class*="align"],
img[class*="wp-image-"] {
	height: auto; /* Make sure images with WordPress-added height and width attributes are scaled correctly */
}
.site-header img,
.entry-content img,
img.size-full {
	max-width: 100%;
	width: auto; /* Prevent stretching of full-size images with height and width attributes in IE8 */
}
.entry-content img.wp-smiley,
.comment-content img.wp-smiley {
	border: none;
	margin-bottom: 0;
	margin-top: 0;
	padding: 0;
}
.wp-caption {
	border: 1px solid #ccc;
	max-width: 100%;
}
.wp-caption.aligncenter,
.wp-caption.alignleft,
.wp-caption.alignright {
	margin-bottom: 1.5em;
}
.wp-caption img {
	display: block;
	margin: 1.2% auto 0;
	max-width: 98%;
}
.wp-caption-text {
	text-align: center;
}
.wp-caption .wp-caption-text {
	margin: 0.8075em 0;
}
.site-content .gallery {
	margin-bottom: 1.5em;
}
.gallery-caption {
}
.site-content .gallery a img {
	border: none;
	height: auto;
	max-width: 90%;
}
.site-content .gallery dd {
	margin: 0;
}
.site-content .gallery-columns-4 .gallery-item {
}
.site-content .gallery-columns-4 .gallery-item img {
}

/* Make sure embeds and iframes fit their containers */
embed,
iframe,
object {
	max-width: 100%;
}


/* =Navigation
----------------------------------------------- */

.site-content .site-navigation {
	margin: 0 0 1.5em;
	overflow: hidden;
}
.site-content .nav-previous {
	float: left;
	width: 50%;
}
.site-content .nav-next {
	float: right;
	text-align: right;
	width: 50%;
}

/* =Comments
----------------------------------------------- */

.bypostauthor {
}

/* =Widgets
----------------------------------------------- */

.widget {
	margin: 0 0 1.5em;
}

/* Search widget */
#searchsubmit {
	display: none;
}
